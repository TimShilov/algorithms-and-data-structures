{
  "name": "algorithms-and-data-structures",
  "version": "0.0.0-development",
  "description": "algorithms-and-data-structures",
  "main": "index.js",
  "scripts": {
    "test": "jest __tests__",
    "test:coverage": "jest __tests__ --coverage",
    "test:watch": "jest __tests__ --watch",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TimShilov/algorithms-and-data-structures.git"
  },
  "keywords": [
    "algorithms",
    "datastructures"
  ],
  "author": "Tim Shilov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TimShilov/algorithms-and-data-structures/issues"
  },
  "homepage": "https://github.com/TimShilov/algorithms-and-data-structures#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "semantic-release": "^17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js|jsx|json|ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/*.{js,jsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
